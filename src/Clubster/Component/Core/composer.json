{
    "name": "kjueni/clubster-core",
    "type": "library",
    "description": "Core handling for PHP applications.",
    "keywords": [
        "core"
    ],
    "homepage": "https://github.com/kjueni",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin JÃ¼ni",
            "homepage": "https://github.com/kjueni"
        }
    ],
    "require": {
        "php": "^7.2",
        "doctrine/collections": "^1.1",
        "symfony/polyfill-mbstring": "^1.6",
        "symfony/security": "^3.4|^4.1.1",
        "sylius/resource": "^1.4"
    },
    "require-dev": {
    },
    "suggest": {
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Clubster\\Component\\Core\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Clubster\\Component\\Core\\spec\\": "spec/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "master"
        }
    }
}
